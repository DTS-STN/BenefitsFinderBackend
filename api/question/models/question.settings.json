{
  "kind": "collectionType",
  "collectionName": "questions",
  "info": {
    "name": "Question"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name_en": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 1
    },
    "name_fr": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 1
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "radio",
        "select"
      ],
      "required": true
    },
    "description_en": {
      "type": "richtext",
      "required": false
    },
    "description_fr": {
      "type": "richtext"
    },
    "next_questions": {
      "collection": "question",
      "via": "previous_question"
    },
    "previous_question": {
      "model": "question",
      "via": "next_questions"
    },
    "display_name_en": {
      "type": "string"
    },
    "display_name_fr": {
      "type": "string"
    },
    "options": {
      "via": "questions",
      "collection": "options"
    },
    "option_question_followings": {
      "via": "question_following_option",
      "collection": "options"
    }
  }
}
